<div class="row">
  <div class="col-lg-12">
    <div class="panel panel-default">
      <div class="panel-heading">
        <div class="row">
          <div class="col-md-12">
            <svg class="glyph stroked pencil"><use xlink:href="#stroked-pencil"/></svg> Usuarios
          </div>
        </div>
      </div>
      <div class="panel-body">

        <table data-toggle="table" id="table-import" data-search="true">
          <thead>
              <th>ID</th>
              <th>Matricula</th>
              <th>Operações</th>
          </thead>

          <tbody>
            <% @users.each do |user| %>
            <tr>
              <td><%= user.id %></td>
              <td><%= user.registration %></td>
              <td>
                <%= (link_to 'Editar permissões', edit_admin_user_path(user), class: "btn btn-info" ) %>
              </td>
            </tr>
            <% end %>
          </tbody>
        </table>
        <nav>
        <ul class="pager">
           <%= page_entries_info @users %>
           <br>
          <li><%= will_paginate @users, :page_links => false, :container => false %></li>
        </ul>
      </nav>

      </div>
    </div>
  </div>
</div>
