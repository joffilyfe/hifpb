<div class="row">
  <div class="col-lg-12">
    <div class="panel panel-default">
      <div class="panel-heading">
        <div class="row">
          <div class="col-md-12">
            <svg class="glyph stroked clipboard with paper"><use xlink:href="#stroked-clipboard-with-paper"/></svg>
            Relatórios
          </div>
        </div>
      </div> <!-- panel-heading -->

      <div class="panel-body">
        <div class="row">
          <div class="col-md-3">Cursos</div>
          <div class="col-md-9">
            <a class="btn btn-default" href="#">
              <span>Visualizar</span>
            </a>
          </div>
        </div>

        <div class="row">
          <div class="col-md-3">Salas</div>
          <div class="col-md-9">
            <%= link_to admin_reports_schoolrooms_path, class: "btn btn-default" do %>
              <span>Visualizar</span>
            <% end %>
          </div>
        </div>

        <div class="row">
          <div class="col-md-3">Laboratórios</div>
          <div class="col-md-9">
            <%= link_to admin_reports_laboratories_path, class: "btn btn-default" do %>
              <span>Visualizar</span>
            <% end %>
          </div>
        </div>

        <div class="row">
          <div class="col-md-3">Professores</div>
          <div class="col-md-9">
            <%= link_to admin_reports_teachers_path, class: "btn btn-default" do %>
              <span>Visualizar</span>
            <% end %>
          </div>
        </div>
      </div> <!-- panel-body -->

    </div>
  </div>
</div>
